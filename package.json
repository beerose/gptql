{
  "name": "gptql",
  "version": "0.0.1",
  "description": "An OpenAI-powered GraphQL Client",
  "author": {
    "email": "alexsandra.sikora@gmail.com",
    "name": "Aleksandra Sikora",
    "url": "https://aleksandra.codes"
  },
  "homepage": "https://github.com/beerose/gptqlblob/main/README.md",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --treeshake --minify",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src"
  },
  "keywords": [
    "graphql",
    "openai",
    "gpt",
    "client"
  ],
  "license": "MIT",
  "dependencies": {
    "openai": "^3.3.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "devDependencies": {
    "@edgeandnode/eslint-config": "^2.0.2",
    "@types/node": "^20.4.10",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "prettier": "^3.0.1",
    "tsup": "^7.2.0"
  }
}